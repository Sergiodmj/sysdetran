// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Processos {
  id     Int      @id @default(autoincrement())
  nome   String
  tipo   String
  datahj DateTime
  numero String   @unique
  status String
  obs    String
}
